{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["google"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "require-jsdoc": 0,
    "spaced-comment": 0,
    "max-len": "off",
    "object-curly-spacing": ["error", "always"],
    "indent": ["error", 2, { "SwitchCase": 1 }]
  }
}
